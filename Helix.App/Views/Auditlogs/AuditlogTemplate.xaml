<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:models="clr-namespace:Helix.App.Models"
             x:DataType="models:AuditlogDisplay"
             x:Class="Helix.App.Views.Auditlogs.AuditlogTemplate">

    <Grid ColumnDefinitions="*, 250" ColumnSpacing="5">
        <Label x:Name="Message" HorizontalOptions="Start" />
        <Label x:Name="Date" Grid.Column="1" HorizontalOptions="End"  />
    </Grid>

</ContentView>
